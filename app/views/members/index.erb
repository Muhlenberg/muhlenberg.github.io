	 <div class="container">
	 <div class="col s12 m6">
    	 <div class="card indigo darken-4">
          	<div class="card-content white-text">
      	 		  <h5 class="center-align"> <%= t.name %> </h5>
      	 		  <p class="center-align"> <%= t.role %> </p>
